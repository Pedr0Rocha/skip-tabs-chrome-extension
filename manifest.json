{
	"name": "Skip Tabs",
	"version": "1.0",
	"author": "Pedro Rocha",
	"description": "Skip pinned tabs when tab-surfing",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Press Ctrl+Q/Command+Q to change tabs and skip pinned ones",
		"default_icon": "icon.png"
	},
	"commands": {
		"next-tab": {
			"suggested_key": {
				"default": "Ctrl+Q",
				"mac": "Command+Q"
			},
			"description": "Next Tab"
		}
	},
	"manifest_version": 2
}